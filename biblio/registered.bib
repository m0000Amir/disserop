@Patent{progbib_mukhtarpov,
  heading =      {Свидетельство о государственной  регистрации программы для {ЭВМ}},
  author =       {\textbf{А. А. Мухтаров}},
  title =        {Программный комплекс для расчета задачи оптимального размещения базовых станций},
  media =        {Программа предназначена для расчета задачи оптимального размещения базовых станций беспроводной широкополосной сети с линейного топологией. Задача представляет собой комбинаторную модель в экстремальной форме. Предложен алгоритм типа ветвей и границ, учитывающий специфику задачи размещения базовых станций телекоммуникационной сети и его реализация в виде программы для ЭВМ. Программа используется в области проектировании телекоммуникационных сетей связи на этапе синтеза топологии. Функциями программы являются поиск максимального телекоммуникационного покрытия сети на заданном участке, выбор мест размещения базовых станций, расчет дальности связи и оценка характеристик производительности сети с помощью стохастической модели. Тип ЭВМ: ВМ PC - совместимый ПК; ОС: MacOS, Linux, Windows.},
  holder =       {Федеральном государственном бюджетном учреждении науки Институт проблем управления им. В.А. Трапезникова Российской академии наук},
  reqnumber =    2022612066,
  publdate =     {2022-02-15},
  date =         {2022-03-01},
  prnumber =     2022613213,
  prcountry =    {Российская Федерация},
  language =     {russian},
  authorprogram ={yes},
}
   
