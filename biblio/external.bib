pages = {634--644},
title = {{Efficient mobile base station placement for first responders in public safety networks}},
volume = {70},
year = {2020}
}
@article{Bendel2018,
abstract = {We consider a tandem network of queues with a Poisson arrival process to the first queue. Service times are assumed to be exponential. In cases where they are not, we additionally assume a processor sharing service discipline in all servers. Consecutive servers may cooperate by pooling resources which leads to the formation of a single combined server that satisfies the aggregated service demands with a greater service rate. On this basis we define a cooperative game with transferable utility, where the cost of a coalition is the steady-state mean total number of customers in the system formed by its members. We show that the game is subadditive, leading to full cooperation being socially optimal. We then show the non-emptiness of the core, despite the characteristic function being neither monotone, nor concave. Finally, we derive several well-known solution concepts, including the Shapley value, the Banzhaf, value and the nucleolus, for the case where servers have equal mean service demands. In particular, we show that all three values coincide in this case.},
author = {Bendel, Dan and Haviv, Moshe},
doi = {10.1016/j.ejor.2018.04.049},
issn = {03772217},
journal = {European Journal of Operational Research},
keywords = {Cooperative games,Core,Cost allocation,Queueing,Shapley value,Tandem queues},
mendeley-groups = {On Optimal Placement of Base Stations inWireless Broadband Networks to Control aLinear Section with end-to-end Delay Limited,Диссертация/External},
number = {3},
pages = {926--933},
title = {{Cooperation and sharing costs in a tandem queueing network}},
volume = {271},
year = {2018}
}
