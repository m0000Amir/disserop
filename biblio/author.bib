@article{Ivanov2019,
abstract = {The paper describes a special case of a base station placement in wireless network with linear topology. Each station is equipped with access point (for example IEEE 802.11) that is used by the objects to send their data via the network, and with a relay equipment that allows the station to connect to the neighboring stations. Each station is described by the coverage radius and the communication radius to other stations. The goal is to determine such placement of the stations which will maximize the total coverage on the linear section by the given set of stations. Two formal problem statements are described and analyzed: the statement in the extremal combinatorial form and the formulation of the problem as a mixed-integer linear programming model. The main result is the development of a special branch and bound algorithm for solving the problem represented in the combinatorial form. The results of a comparative computational experiment by three approaches: by the brute force method, by the branch and bound algorithm and solving the problem in the form of the mixed-integer linear programming are given.},
author = {Ivanov, R. and Mukhtarov, A. and Pershin, O.},
doi = {10.1007/978-3-030-36625-4_5},
isbn = {9783030366247},
issn = {18650937},
journal = {Communications in Computer and Information Science},
keywords = {Branch and bound method,Combinatorial model,Optimal base stations placement,Wireless network},
mendeley-groups = {Диссертация/Мои статьи},
pages = {53--64},
title = {{A Problem of Optimal Location of Given Set of Base Stations in Wireless Networks with Linear Topology}},
volume = {1141 CCIS},
year = {2019},
authorscopus = {true},
addendum = {(Scopus, WoS)},
language = {english},
}

@article{Mukhtarov2020,
abstract = {The paper is dedicated to the design problem of a wireless communication network. The problem of base stations optimal placement along a linear section subject to control with end-to-end delay limited is formulated. The goal is to maximize the coverage area that comes to under the control of multiple deployed base stations while respecting technological conditions and budget constraints. In the paper, the features of the technological statement are analyzed. The formulation of the problem in the form of integer linear programming is proposed. When forming a mathematical model, a queuing network model with a Poisson arrival process is used.},
author = {Mukhtarov, Amir and Pershin, Oleg and Larionov, Andrey and Vishnevsky, V. M.},
doi = {10.1007/978-3-030-66242-4_3},
isbn = {9783030662417},
issn = {18650937},
journal = {Communications in Computer and Information Science},
keywords = {Integer linear programming,Poisson flow,Wireless network},
mendeley-groups = {Диссертация/Мои статьи},
pages = {30--42},
title = {{On Optimal Placement of Base Stations in Wireless Broadband Networks to Control a Linear Section with End-to-End Delay Limited}},
volume = {1337},
year = {2020},
authorscopus = {true},
language = {english},
}


@article{IvanovVAK2019,
abstract = {В настоящее время в промышленных системах сбора информации с контролируемой территории все большее распространение получают беспроводные сети широкополосной связи. Существенное значение для эффективного проектирования таких сетей имеет выбор оптимального размещения заданного множества базовых станций, обладающих различными характеристикамиrticle, позволяющий осуществить покрытие максимальной по размеру территории при выполнении технологических требований. В статье рассматривается актуальный специальный случай задачи размещения базовых станций беспроводной сети связи для контроля линейной (одномерной) территории. В нефтегазовой отрасли примерами таких объектов могут служить линейные участки магистральных трубопроводов, коридоры промысловых коммуникаций, автомобильные трассы и т. п. В статье дается математическая формулировка задачи в виде экстремальной комбинаторной модели. Анализируется специфика модели и на ее основе предлагается алгоритм метода ветвей и границ (МВиГ). Рассмотрен числовой пример, позволяющий показать преимущества предложенного алгоритма МВиГ для решения данной задачи. Алгоритмы запрограммированы на языке Python и в статье приводятся результаты вычислительного эксперимента.},
author = {Иванов, Р. Е. and Мухтаров, А. А. and Першин, О.Ю.},
issn = {01322222},
journal = {Автоматизация, телемеханизация и связь в нефтяной промышленности},
keywords = {беспроводные сети связи,границ,комбинаторная модель,метод ветвей и,оптимальное размещение базовых станций},
mendeley-groups = {Диссертация/Мои статьи},
number = {4},
pages = {39--45},
title = {Задача оптимального размещения заданного множества базовых станций беспроводной сети связи с линейной топологией},
volume = {549},
year = {2019},
doi = {10.33285/0132-2222-2019-4(549)-39-45},
language = {russian},
authorvak = {true},
} 

